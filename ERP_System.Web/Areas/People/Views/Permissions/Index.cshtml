                <div class="form-group  min-w-3">
                    <div class="form-group  min-w-3">
                        <label class="lable" for="ddlRegions">نوع المستخدم</label>
                        <select onchange="ShowUserPermission()" class="Select sAddress form-select" aria-label="اختر المنطقة" asp-items='new SelectList((IEnumerable<SelectListDTO>)ViewData["UserTypeId"], "Value", "Text")' name="UserTypeId" id="ddlUserType">
